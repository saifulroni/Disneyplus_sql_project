        "    mask = (y == 0) & (initial_labels_by_class[0] == cluster_id)\n",
        "    X_sub = X.loc[mask]\n",
        "\n",
        "    if X_sub.shape[0] < 2:\n",
        "        print(f\"  Skipping reclustering for cluster {cluster_id}: fewer than 2 samples.\")\n",
        "        continue\n",
        "\n",
        "    # Determine a reasonable number of sub-clusters (e.g., 2 or 3, or based on subset size)\n",
        "    # For demonstration, let's try to find 2 sub-clusters if possible\n",
        "    n_sub_clusters = min(3, X_sub.shape[0] -1) # Try up to 3 sub-clusters, but at least 2 samples needed for 2 clusters\n",
        "\n",
        "    if n_sub_clusters < 2:\n",
        "         print(f\"  Skipping reclustering for cluster {cluster_id}: not enough samples for at least 2 sub-clusters.\")\n",
        "         continue\n",
        "\n",
        "    # Run spectral clustering on the subset\n",
        "    # Use k_range=(2, n_sub_clusters + 1) to search up to n_sub_clusters\n",
        "    sub_sub_labels, chosen_k, score, _ = run_spectral_clustering(\n",
        "        X_sub,\n",
        "        class_label=f\"0 (Initial Cluster {cluster_id})\",\n",
        "        k_range=(2, n_sub_clusters + 1) # Search for 2 to n_sub_clusters\n",
